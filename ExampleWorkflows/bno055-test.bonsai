          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="Zip" />
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="q1:UpdateUniform">
            <q1:UniformName>projection</q1:UniformName>
            <q1:ShaderName>PerPixelLight</q1:ShaderName>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="q1:CreateQuaternionRotation">
            <q1:Rotation>
              <q1:Xyz>
                <q1:X>0</q1:X>
                <q1:Y>0</q1:Y>
                <q1:Z>0</q1:Z>
              </q1:Xyz>
              <q1:W>1</q1:W>
            </q1:Rotation>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="q1:CreateLookAt">
            <q1:Eye>
              <q1:X>0</q1:X>
              <q1:Y>0</q1:Y>
              <q1:Z>5</q1:Z>
            </q1:Eye>
            <q1:Target>
              <q1:X>0</q1:X>
              <q1:Y>0</q1:Y>
              <q1:Z>0</q1:Z>
            </q1:Target>
            <q1:Up>
              <q1:X>0</q1:X>
              <q1:Y>1</q1:Y>
              <q1:Z>0</q1:Z>
            </q1:Up>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="q1:CreatePerspectiveFieldOfView">
            <q1:FovY>1.04719758</q1:FovY>
            <q1:AspectRatio>1</q1:AspectRatio>
            <q1:NearClip>0.1</q1:NearClip>
            <q1:FarClip>1000</q1:FarClip>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="q1:UpdateFrame" />
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="ExternalizedPropertyOfQuaternionCreateQuaternionRotation">
          <MemberName>Rotation</MemberName>
          <Name>Rotation</Name>
          <Value>
            <Xyz>
              <X>0</X>
              <Y>0</Y>
              <Z>0</Z>
            </Xyz>
            <W>1</W>
          </Value>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="q1:UpdateFrame" />
        </Builder>
      </Expression>
      <Expression xsi:type="q2:PythonTransform">
        <q2:Name>Heading</q2:Name>
        <q2:Script>import clr
clr.AddReference("OpenCV.Net")
from OpenCV.Net import *
from math import atan2
@returns(float)
def process(value):

  q0 = value[0].Val0;
  q1 = value[1].Val0;
  q2 = value[2].Val0;
  q3 = value[3].Val0;

  return atan2((2 * (q0 * q3 + q1 * q2)), (1 - 2 * (q2**2 + q3**2)))</q2:Script>
      </Expression>
      <Expression xsi:type="Combinator">
        <Combinator xsi:type="q3:BNO055Device">
          <q3:DeviceIndex>
            <q3:SelectedIndex>3</q3:SelectedIndex>
            <q3:Indices>
              <q3:int>3</q3:int>
            </q3:Indices>
          </q3:DeviceIndex>
        </Combinator>
      </Expression>
